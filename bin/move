#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts("Welcome to Tic Tac Toe!")
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts("Where would you like to go?")
action = gets.strip
actual_move = input_to_index(action)
move(board, actual_move, "X")
display_board(board)
