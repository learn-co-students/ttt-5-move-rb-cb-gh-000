#!/usr/bin/env ruby
require_relative '../lib/move.rb'

board = [" "," "," "," "," "," "," "," "," "]


puts "Welcome to Tic Tac Toe!"#Okay
display_board(board)
puts"Where would you like to go?"#Okay

user_input=gets.strip
index = input_to_index(user_input)

puts"Please choose your character"

move(board, index)#Okay

display_board(board) #Okay
