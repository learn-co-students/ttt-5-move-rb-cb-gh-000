#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts  "Welcome to Tic Tac Toe!"

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Where would you like to go?"

def input_to_index(input)
  input.to_i - 1
end
def move(array, index, value = 'X')
  array[index] = value
end

input = gets.strip
index = input_to_index(input)
move(board, index)
display_board(board)
