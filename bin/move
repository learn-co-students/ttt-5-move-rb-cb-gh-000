#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board=[" "," "," "," "," "," "," "," "," "]
#board=Array.new['   ','   ','   ','   ','   ','   ','   ','   ','   ']
puts "Where would you like to go?"
input=gets.strip
pos=input_to_index(input)
move(board,pos)
display_board(board)
