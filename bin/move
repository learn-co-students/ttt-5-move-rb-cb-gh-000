#!/usr/bin/env ruby

require_relative '../lib/move.rb'
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " "," ", " ", " "," ", " ", " "]
display_board(board)
puts "Where would you like to go?"
place=gets.strip
index=input_to_index(place)
move(board,index,"X")
display_board(board)
