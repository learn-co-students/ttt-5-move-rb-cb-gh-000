#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts"Welcome to Tic Tac Toe!"
board=[" "," "," "," "," "," "," "," "," "]
puts"   |   |   "
puts"-----------"
puts"   |   |   "
puts"-----------"
puts"   |   |   "

puts"Where would you like to go?"
input=gets.strip

move(board,input_to_index(input),char="X")
display_board(board)
