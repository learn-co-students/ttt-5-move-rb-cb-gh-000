#!/usr/bin/env ruby

require_relative '../lib/move.rb'








# Code your CLI Here
puts "Welcome to Tic Tac Toe!"

puts "Where would you like to go?"
user_input = gets.strip
input = input_to_index(user_input)
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
move(board, input, value="X")

display_board(board)
