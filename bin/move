#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" "," "," "," "," "," "," "," "," "]
puts "Where would you like to go?"
position = gets.strip
converted_input = input_to_index(position)
move(board,converted_input)
display_board(board)
