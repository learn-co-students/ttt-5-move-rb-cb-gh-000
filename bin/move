#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Where would you like to go?"

input = gets.strip

converted = input_to_index(input)

move(board, converted)

display_board(board)
