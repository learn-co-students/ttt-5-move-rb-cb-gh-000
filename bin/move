#!/usr/bin/env ruby
require_relative '../lib/move.rb'
# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Where would you like to go?"
user_input = gets.strip
user_input = input_to_index(user_input)
move(board, user_input, "X")
display_board(board)
