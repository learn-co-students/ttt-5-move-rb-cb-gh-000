#!/usr/bin/env ruby
require_relative "../lib/move.rb"

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Enter a number from 1-9 for which square you want"
user_input = gets.strip
index = input_to_index(user_input)
move(board, index)
display_board(board)
